<odoo>
<record id="model_resource_booking" model="ir.model">
    <field name="name">resource.booking</field> <!-- Add this -->
    <field name="model">x_resource_booking</field> <!-- Add this -->
    <field name="website_form_key">create_reservation</field>
    <field name="website_form_label">Create Reservation</field>
    <field name="website_form_access">True</field>
    <field name="website_form_default_field_id" ref="resource_booking.model_resource_booking"/>
</record>

    <function model="ir.model.fields" name="formbuilder_whitelist">
        <value>resource.booking</value>
        <value eval="[
            'name',
            'start',
            'stop',
            'description',
            'partner_ids',
        ]"/>
    </function>

