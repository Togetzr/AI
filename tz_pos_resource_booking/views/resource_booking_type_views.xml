<odoo>
    <!-- Views -->
    <record id="resource_booking_type_view_form_inherit" model="ir.ui.view">
        <field name="name">Resource booking type form</field>
        <field name="model">resource.booking.type</field>
        <field name="inherit_id" ref="resource_booking.resource_booking_type_view_form"/>
        <field name="arch" type="xml">
            <field name="company_id" position="before">
                <field name="product_id"/>
                <field name="price"/>
                <field name="currency_id" invisible="1"/>
            </field>
        </field>
    </record>
</odoo>
