<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record model="ir.ui.view" id="res_company_inherit">
        <field name="name">res.company.inherited</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="tz_whatsapp_integration.res_company_inherit"/>
        <field name="arch" type="xml">
            <xpath expr="//header" position="inside">
                <button type="object" name="sync_flows_to_facebook" class="oe_highlight" string="Sync Flows Details"/>
            </xpath>
            <field name="website" position="after">
                <field name="resource_tz"/>
            </field>
        </field>
    </record>
</odoo>
