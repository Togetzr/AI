<templates id="template" xml:space="preserve">
    <t
        t-name="Navbar"
        t-inherit="point_of_sale.Navbar"
        t-inherit-mode="extension"
        owl="1"
    >
        <xpath expr="//div[hasclass('pos-branding')]/img" position="replace">
                <img class="pos-logo h-75 ms-3 me-auto align-self-center" t-on-click="() => debug.toggleWidget()" t-attf-src="/web/image/res.company/{{this.pos.company.id}}/logo" alt="Logo" />
        </xpath>
    </t>
</templates>
